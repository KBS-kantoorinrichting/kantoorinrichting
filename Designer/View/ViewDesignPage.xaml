<Page x:Class="Designer.View.ViewDesignPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Designer.View"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ViewDesignPage">

    <Grid>
        <TabControl x:Name="Tabs" Margin="540,0,0,0" HorizontalAlignment="Left" Width="260">
            <TabItem Header="Catalogus">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Overzicht">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>
        <Canvas x:Name="Editor" Margin="0,0,265,0"/>

    </Grid>
</Page>
